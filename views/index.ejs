                    data-slider-options='{"breakpoints":{"0":{"slidesPerView":1},"576":{"slidesPerView":"1"},"768":{"slidesPerView":"2"},"992":{"slidesPerView":"2"},"1200":{"slidesPerView":"2"}}}'>
                    <div class="swiper-wrapper">
                        <% if (blogs && blogs.length > 0) { %>
                            <% blogs.forEach(function(blog) { %>
                                <div class="swiper-slide">
                                    <div class="blog-card style5">
                                        <div class="blog-img">
                                            <a href="/blogs/<%= blog._id %>">
                                                <img src="<%= blog.imageUrl %>" alt="blog image">
                                            </a>
                                        </div>
                                        <div class="blog-content">
                                            <div class="blog-meta">
                                                <a href="/blogs/<%= blog._id %>">
                                                    <%= blog.createdAt ? blog.createdAt.toLocaleDateString('en-US', { year: 'numeric', month: 'long', day: 'numeric' }) : '' %>
                                                </a>
                                                <% /* Optionally, you can display reading time if available, or estimate it */ %>
                                                <% if (blog.readTime) { %>
                                                    <a href="/blogs/<%= blog._id %>"><%= blog.readTime %> min read</a>
                                                <% } %>
                                            </div>
                                            <h3 class="box-title">
                                                <a href="/blogs/<%= blog._id %>"><%= blog.title %></a>
                                            </h3>
                                            <a href="/blogs/<%= blog._id %>" class="th-btn style-border2 th-btn-icon">Read More</a>
                                        </div>
                                    </div>
                                </div>
                            <% }); %>
                        <% } else { %>
                            <div class="swiper-slide">
                                <div class="blog-card style5">
                                    <div class="blog-content">
                                        <h3 class="box-title">No blog posts found.</h3>
                                    </div>
                                </div>
                            </div>
                        <% } %>
                    </div>
                </div>
            </div>
        </div>
